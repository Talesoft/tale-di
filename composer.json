{
    "name": "talesoft/tale-di",
    "type": "library",
    "keywords": ["di", "container", "psr-11"],
    "description": "A PSR-15 compatible HTTP utility library",
    "license": "MIT",
    "homepage": "http://docs.talesoft.codes/php/tale/di",
    "authors": [
        {
            "name": "Torben Koehn",
            "email": "torben@talesoft.codes"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "talesoft/tale-cache-core": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Tale\\": "src",
            "Tale\\Test\\": "tests"
        },
        "functions": ["src/functions.php"]
    },
    "provides": {
        "psr/container-implementation": "~1.0"
    },
    "require-dev": {
        "talesoft/tale-dev-tool": "dev-master"
    }
}
