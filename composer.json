{
    "name": "talesoft/tale-di",
    "type": "library",
    "keywords": [
        "di",
        "container",
        "container-builder",
        "dependency-injection",
        "dependencies",
        "services",
        "psr-11"
    ],
    "description": "A PSR-11 compatible dependency injection library",
    "license": "MIT",
    "homepage": "http://docs.talesoft.codes/php/tale/di",
    "authors": [
        {
            "name": "Torben KÃ¶hn",
            "email": "torben@talesoft.codes"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "test:coverage-html": "phpunit --coverage-html=coverage",
        "lint": "phpcs",
        "lint:fix": "phpcbf"
    },
    "require": {
        "php": ">=7.1.0",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "talesoft/tale-cache-core": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Tale\\": "src"
        },
        "functions": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tale\\Test\\": "tests"
        }
    },
    "provide": {
        "psr/container-implementation": "~1.0"
    }
}
